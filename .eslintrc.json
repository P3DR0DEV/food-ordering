{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "@rotrenanoliveira/eslint-config/next"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "react"
  ],
  "rules": {
    "jsx-a11y/alt-text": "off",
    "no-useless-constructor": "off",
    "camelcase": "off",
    "react/react-in-jsx-scope": "off"
  }
}